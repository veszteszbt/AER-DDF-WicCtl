
%token NR

%%

lines:
    lines line  // 1
|
                // 2
;

line:
    content     // 3
    '\n'
    {
        std::cout << "Line token newline at " << @2.first_line << "\n";
    }
;

content:
    expr        // 4
|
    error       // 5
|
    // empty
;

expr:
    NR              // 7
    {
        std::cout << "NR returns value " << $1 << "\n";
    }
;

