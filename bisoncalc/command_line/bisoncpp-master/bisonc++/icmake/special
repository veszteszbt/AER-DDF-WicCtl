void special()
{
    if ("INSTALL.im" newer "options/SKEL")
        run("echo \"#define _Skel_  \\\"" SKEL + "\\\"\" > options/SKEL");

    if (! exists("release.yo") || "VERSION" newer "release.yo") 
    {
        system("touch version.cc");
        run("gcc -E VERSION.h | grep -v '#' | sed 's/\\\"//g' > "
                                                            "release.yo");
    }
}
